{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": 10,
          "edge": 17,
          "firefox": 60,
          "chrome": 67,
          "safari": 11
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "compact": false,
   "plugins": [
     ["@babel/plugin-proposal-decorators", { "legacy": true }],
     ["@babel/plugin-proposal-class-properties", { "loose": true }],
     ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true}, "ant"],
     ["import", { "libraryName": "@ant-design/charts", "libraryDirectory": "es"}, "charts"],
     "@babel/plugin-transform-runtime"
   ]
}